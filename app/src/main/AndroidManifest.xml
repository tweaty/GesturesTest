<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tweaty.gesturestest" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SurveyActivity"
            android:label="@string/title_activity_survey"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tweaty.gesturestest.MainActivity" />
        </activity>
        <activity
            android:name=".TapActivity"
            android:label="@string/title_activity_tap"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".TestListActivity"
            android:label="@string/title_activity_test_list"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".PanActivity"
            android:label="@string/title_activity_pan"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ZoomActivity"
            android:label="@string/title_activity_zoom"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SequenceActivity"
            android:label="@string/title_activity_sequence"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>
